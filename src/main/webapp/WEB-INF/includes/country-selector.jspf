<%@ page import="java.util.Collections" %>
<%@ page import="java.util.ArrayList" %>
<%@ page import="java.util.List" %>
<% final String LANG_PACK = "country."; %>

<div class="div-label"><label for="country"><%= langBundle.getString(LANG_PACK + "country") %></label><span class="required"></span></div>
<select id="country" name="country" required>
    <%
            List<String> keys = new ArrayList<>(langBundle.keySet());
            Collections.sort(keys);

        for (String key : keys) {
            if (key.startsWith(LANG_PACK)) {
    %>
    <option value="<%= key.substring(LANG_PACK.length()) %>"><%= langBundle.getString(key) %></option>
    <%      }
        }
    %>
</select>